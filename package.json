{
  "name": "@reshare/platform-rs",
  "version": "1.12.0",
  "description": "Top-level platform for ReShare UI",
  "license": "Apache-2.0",
  "repository": "openlibraryenvironment/platform-rs",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "scripts": {
    "build": "export STRIPES_TRANSPILE_TOKENS=\"@k-int @reshare\"; export NODE_OPTIONS=\"--max-old-space-size=4096 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "cypress": "cypress",
    "build-module-descriptors": "node ./build-module-descriptors.js",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
	"start-local-diku": " set STRIPES_TRANSPILE_TOKENS=\"@k-int @reshare\" && stripes serve ./stripes.config.js --okapi=http://localhost:9130 --tenant=diku"
  },
  "dependencies": {
    "@folio/developer": "~6.3.0",
    "@folio/stripes": "~7.3.0",
    "@folio/tenant-settings": "~7.3.0",
    "@folio/users": "~8.2.0",
    "@reshare/directory": "openlibraryenvironment/ui-directory#release-1.10.x",
    "@reshare/plugin-rs-siquery-vufind": "openlibraryenvironment/ui-plugin-rs-siquery-vufind#release-v1.0.x",
    "@reshare/plugin-rs-siquery-folio": "openlibraryenvironment/ui-plugin-rs-siquery-folio#release-1.3.x",
    "@reshare/request": "openlibraryenvironment/ui-request#release-1.6.x",
    "@reshare/rs": "openlibraryenvironment/ui-rs#release-1.13.x",
    "@reshare/stripes-reshare": "openlibraryenvironment/stripes-reshare#release-1.10.x",
    "@reshare/supply": "openlibraryenvironment/ui-supply#release-1.6.x",
    "@reshare/update": "openlibraryenvironment/ui-update#release-1.8.x",
    "moment": "~2.29.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-intl": "^5.21.0",
    "react-query": "^3.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "rrule": "2.6.4"
  },
  "resolutions": {
    "@folio/stripes-testing": "4.2.1",
    "@reshare/plugin-rs-siquery-vufind": "openlibraryenvironment/ui-plugin-rs-siquery-vufind#release-v1.0.x",
    "@reshare/plugin-rs-siquery-folio": "openlibraryenvironment/ui-plugin-rs-siquery-folio#release-1.3.x",
    "@reshare/request": "openlibraryenvironment/ui-request#release-1.6.x",
    "@reshare/rs": "openlibraryenvironment/ui-rs#release-1.13.x",
    "@reshare/supply": "openlibraryenvironment/ui-supply#release-1.6.x",
    "@reshare/update": "openlibraryenvironment/ui-update#release-1.8.x",
    "@reshare/stripes-reshare": "openlibraryenvironment/stripes-reshare#release-1.10.x"
  },
  "devDependencies": {
    "@folio/stripes-cli": "~2.6.0",
    "@folio/stripes-testing": "4.2.1",
    "@interactors/html": "^1.0.0-rc1.2",
    "@interactors/with-cypress": "^1.0.0-rc1.1",
    "cypress": "^9.6.1"
  }
}
