    "@folio/stripes": "6.2.2",
    "@reshare/directory": "1.4.0",
    "@reshare/plugin-rs-siquery-folio": "1.1.0",
    "@reshare/request": "1.1.1",
    "@reshare/rs": "1.6.0",
    "@reshare/supply": "1.1.4",
    "@reshare/update": "1.4.0",
    "moment": "~2.29.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^5.7.0",
    "react-redux": "^7.2.2",
    "react-query": "^3.13.0",
