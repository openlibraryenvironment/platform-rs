    "@reshare/directory": "openlibraryenvironment/ui-directory#release-1.8.x",
    "@reshare/plugin-rs-siquery-folio": "openlibraryenvironment/ui-plugin-rs-siquery-folio#release-1.3.x",
    "@reshare/request": "openlibraryenvironment/ui-request#release-1.5.x",
    "@reshare/rs": "openlibraryenvironment/ui-rs#release-1.10.x",
    "@reshare/supply": "openlibraryenvironment/ui-supply#release-1.5.x",
    "@reshare/update": "openlibraryenvironment/ui-update#release-1.6.x",
