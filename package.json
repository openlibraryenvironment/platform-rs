{
  "name": "@projectreshare/platform-rs",
  "version": "1.18.2",
  "description": "Top-level platform for ReShare UI",
  "license": "Apache-2.0",
  "repository": "openlibraryenvironment/platform-rs",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "scripts": {
    "build": "export STRIPES_TRANSPILE_TOKENS=\"@k-int @projectreshare\"; export NODE_OPTIONS=\"--max-old-space-size=4096 $NODE_OPTIONS\"; ./node_modules/.bin/stripes build stripes.config.js",
    "stripes": "./node_modules/.bin/stripes",
    "start": "stripes serve stripes.config.js",
    "cypress": "cypress",
    "build-module-descriptors": "node ./build-module-descriptors.js",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
	"start-local-diku": " set STRIPES_TRANSPILE_TOKENS=\"@k-int @projectreshare\" && stripes serve ./stripes.config.js --okapi=http://localhost:9130 --tenant=diku"
  },
  "dependencies": {
    "@folio/checkin": "9.2.1",
    "@folio/checkout": "10.1.0",
    "@folio/circulation": "9.1.0",
    "@folio/circulation-log": "4.1.0",
    "@folio/developer": "8.0.1",
    "@folio/inventory": "11.0.5",
    "@folio/requests": "10.0.2",
    "@folio/stripes": "9.1.8",
    "@folio/stripes-marc-components": "1.0.1",
    "@folio/tenant-settings": "8.1.0",
    "@folio/users": "10.1.2",
    "@projectreshare/directory": "1.13.7",
    "@projectreshare/request": "1.6.5",
    "@projectreshare/rs": "1.20.8",
    "@projectreshare/supply": "1.6.5",
    "moment": "~2.29.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-intl": "^6.4.7",
    "react-query": "^3.13.0",
    "react-redux": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.2.1",
    "rrule": "2.6.4",
    "inflected":" ^2.0.4"
  },
  "resolutions": {
    "colors": "1.4.0",
    "final-form": "^4.20.4",
    "minimist": "^1.2.3",
    "moment": "~2.29.0",
    "redux-form": "^8.0.0",
    "@projectreshare/rs": "1.19.10",
    "@projectreshare/stripes-reshare": "2.0.4"
  },
  "devDependencies": {
    "@folio/stripes-cli": "~3.1.0",
